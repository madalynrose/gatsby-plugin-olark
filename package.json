{
  "name": "gatsby-plugin-olark",
  "version": "1.0.0",
  "description": "Plugin to add Olark chat widget to your site",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "olark",
    "chat"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "babel-preset-gatsby-package": "^0.2.9",
    "cross-env": "^5.2.1"
  },
  "peerDependencies": {
    "gatsby": "^2.0.0"
  }
}
